"vol up"
   XF86AudioRaiseVolume

"vol down"
   XF86AudioLowerVolume

"vol mute"
   XF86AudioMute

"brightness up"
  XF86MonBrightnessUp

"brightness down"
  XF86MonBrightnessDown

# bring up background chooser
"chbg choose"
  Shift+Mod4 + b

# change to a random background
"chbg rand"
  Control+Shift+Mod4 + b

# launch the screenshot utility
"screenshot"
  Print

# launch file manager
"nautilus -w"
  Shift+Mod4 + apostrophe

# launch firefox
"ff"
  Shift+Mod4 + semicolon
